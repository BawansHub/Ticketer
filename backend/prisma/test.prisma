model User {
    id Int @default(autoincrement()) @id
    name String
    email String @unique
    password String
    forSale Ticket[]
    posts Post[]
}

model Ticket {
    id Int @default(autoincrement()) @id
    owner User @relation(fields: [ownerID], references: [id])
    ownerID Int
}

model Post {
    id Int @default(autoincrement()) @id
    owner User @relation(fields: [ownerID], references: [id])
    ownerID Int
}